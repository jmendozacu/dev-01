<?xml version="1.0"?>
<config>
    <modules>
        <Magebuzz_Custom>
            <version>0.1.0</version>
        </Magebuzz_Custom>
    </modules>

    <frontend>
        <routers>
            <custom>
                <use>standard</use>
                <args>
                    <module>Magebuzz_Custom</module>
                    <frontName>custom</frontName>
                </args>
            </custom>
        </routers>
    </frontend>
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                    <catalog_category_tabs>Magebuzz_Custom_Block_Adminhtml_Catalog_Category_Tabs</catalog_category_tabs>
                </rewrite>
            </adminhtml>
        </blocks>
    </global>
</config>
